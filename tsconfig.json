{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Node",
    "types": ["bun-types"],
    "baseUrl": ".",
    "paths": {
      "@app/*": ["src/*"],
      "@commands/*": ["src/commands/*"],
      "@constants/*": ["src/constants/*"],
      "@events/*": ["src/events/*"],
      "@interfaces/*": ["src/interfaces/*"],
      "@managers/*": ["src/managers/*"],
      "@structs/*": ["src/structs/*"],
      "@types/*": ["src/types/*"],
      "@utils/*": ["src/utils/*"]
    }
  },
  "noImplicitAny": false
}
